<% @title = "御社のコメント表示の編集" %>
<h1><%= @title %></h1>

<div id="generic-form">
  <div id="entry">
  <%= form_for @entry_form, as: 'form', url: :client_entry do |f| %>
    <%= render "form", f: f %>
  
    <div class="buttons">
      <%= f.submit '更新' %>
      <%= link_to 'キャンセル', :client_account %>
    </div>
  <% end %>
  </div>
</div>