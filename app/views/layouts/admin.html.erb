<!DOCTYPE html>
<html>
  <head>
    <%= favicon_link_tag 'favicon.ico.png', type: 'image/png' %>
    <%= favicon_link_tag('apple-touch-icon-180.png', {:rel => 'apple-touch-icon', :sizes => '180*180', :type => 'image/png'}) %>
    <%= favicon_link_tag('android-touch-icon-192.png', {:rel => 'android-touch-icon', :sizes => '192*192', :type => 'image/png'}) %>
    <title>Hunter</title>
    <%= csrf_meta_tags %>
    <title><%= document_title %></title>
    <%= stylesheet_link_tag    'admin', media: 'all', 'data-turbolinks-track' => true %>
    <%= javascript_include_tag 'admin', 'data-turbolinks-track' => true %>
  </head>

  <body>
    <div id="wrapper">
    <%= render 'admin/shared/header' %>
    <div id="container">
      <%= yield %>
    </div>
    <%= render 'shared/footer' %>
  </div>
  </body>
</html>
