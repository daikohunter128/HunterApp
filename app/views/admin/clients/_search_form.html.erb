<%= form_for @search_form, as: 'search', url: :admin_clients,
    html: { method: :get, class: 'search' } do |f| %>
 <%= markup do |m|
    p = FormPresenter.new(f, self)
    m << p.text_field_block(:company_name_kana, 'フリガナ（会社名) :')
    m << p.drop_down_list_block(:prefecture, '都道府県:', Client::PREFECTURE_NAMES)
    m << f.submit('検索')
  end %>
<% end %>